function switchSuggest(flag) {
    var url = "http://transit.loco.yahoo.co.jp/search/result?from=%E5%93%81%E5%B7%9D%E3%82%B7%E3%83%BC%E3%82%B5%E3%82%A4%E3%83%89&flatlon=&to=%E8%88%B9%E6%A9%8B%E6%B3%95%E5%85%B8&tlatlon=&viacode=&ym=201412&d=06&hh=18&m1=0&m2=0&shin=&ex=&hb=&al=&lb=&sr=&type=1&ws=1&s=1&ei=&fl=1&tl=3&expkind=1&mtf=&out_y=&mode=&searchOpt=&stype=&ticket=ic&passtype=&c="+flag;
    location.replace(url);
}

